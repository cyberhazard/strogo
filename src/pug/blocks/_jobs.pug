      .jobs__group(data-category='oxfords') Оксфорды
      .jobs__group(data-category='slippers') Слипперы
      .jobs__group(data-category='howlcuts') Хоулкатс
    .jobs__blocks
    .jobs__blocks_template(style="display: none")
      mixin jobs(name, price, img, category, type)
        .jobs__block(class=!type? '' : type=='left'? 'jobs__block_left' : 'jobs__block_right' data-category=category)
          .jobs__image.lg
            - let [folder, qty] = img.split(' ')
            - for(let i = 1 ; i < +qty+1 ; i++)
              a(href=`${folder}/${i}.jpg` style=i>=2? 'display:none;': '')
                img(src="" data-src=`${folder}/${i}.jpg` style="width: 100%")
          .jobs__shape
            include ../../images/shape-hand.svg
          .jobs__label= name
          .jobs__price!= `${price} ${price && '<span>руб.</span>'}`

